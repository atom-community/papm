{
  "name": "papm",
  "main": "dist/main.js",
  "version": "0.1.0",
  "author": "<Your Name>",
  "description": "Performant Atom Package Manager",
  "keywords": ["pnpm", "npm", "apm", "pm", "atom", "package-manager"],
  "repository": "https://github.com/atom-community/papm",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write .",
    "test.format": "prettier . --check",
    "lint": "eslint . --fix",
    "test.lint": "eslint .",
    "test": "node ./script/get-fixtures.js && npm run check-version && jasmine-focused --captureExceptions --coffee spec",
    "clean": "shx rm -rf dist",
    "tsc": "tsc -p src/tsconfig.json || echo done",
    "dev": "npm run clean && cross-env NODE_ENV=development cross-env BABEL_KEEP_MODULES=true rollup -c -w",
    "build": "npm run clean && cross-env NODE_ENV=production cross-env BABEL_KEEP_MODULES=true rollup -c",
    "build-commit": "build-commit -o dist",
    "bump": "ncu -u -x coffeescript",
    "prepare": "npm run tsc && node script/postinstall.js",
    "check-version": "node script/check-version.js"
  },
  "prettier": "prettier-config-atomic",
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "engines": {
    "atom": ">=1.52.0 <2.0.0",
    "electron": ">=6.x"
  },
  "dependencies": {
    "@atom/plist": "0.4.4",
    "asar-require": "0.3.0",
    "async": "^3.2.0",
    "colors": "~1.4.0",
    "first-mate": "^7.4.3",
    "fs-plus": "3.x",
    "git-utils": "^5.7.2",
    "glob": "^7.1.6",
    "hosted-git-info": "^3.0.7",
    "keytar": "^7.7.0",
    "mv": "2.1.1",
    "ncp": "~2.0.0",
    "npm": "^6.14.9",
    "open": "7.3.0",
    "q": "~1.5.1",
    "read": "~1.0.7",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "season": "^6.0.2",
    "semver": "^7.3.4",
    "tar": "^6.0.5",
    "temp": "^0.9.4",
    "underscore-plus": "1.x",
    "wordwrap": "1.0.0",
    "wrench": "~1.5.1",
    "yargs": "^3.32.0"
  },
  "devDependencies": {
    "@types/atom": "1.40.11",
    "@types/jasmine": "^1",
    "@types/node": "^16.4.0",
    "build-commit": "0.1.4",
    "cross-env": "7.0.3",
    "eslint-config-atomic": "^1.16.2",
    "express": "^4.17.1",
    "jasmine-focused": "^1.0.7",
    "node-gyp": "^5.1.1",
    "npm-check-updates": "11.8.3",
    "prettier-config-atomic": "^2.0.5",
    "rollup-plugin-atomic": "^2.4.0",
    "shx": "0.3.3",
    "terser-config-atomic": "^0.1.1",
    "typescript": "^4.3.5"
  },
  "providedServices": {},
  "consumedServices": {},
  "package-deps": []
}
